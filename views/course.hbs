<div class="row">
  <div class="col s6 offset-s3">
    <div class="card">
      <div class="card-image">
        <img src="{{ course.image }}" alt="{{ course.title }}">
      </div>
      <div class="card-content">
        <h3>{{ course.title }}</h3>
        <p class="price">{{ course.price }}</p>
      </div>
    </div>
  </div>
</div>
